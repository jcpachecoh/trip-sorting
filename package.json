{
  "name": "trip-sorter",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/empty-module.js",
      "\\.(css|less)$": "<rootDir>/empty-module.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupConfig.js",
    "automock": false
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-router-redux": "^4.0.8",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^23.10.4",
    "typeface-roboto": "0.0.54"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.3",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.3",
    "@types/react": "^16.4.14",
    "@types/react-bootstrap": "^0.32.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-redux": "^5.0.16",
    "@types/redux-mock-store": "^1.0.0",
    "@types/redux-thunk": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest-fetch-mock": "^1.6.6",
    "react-redux": "^5.0.7",
    "redux-mock-store": "^1.5.3",
    "typescript": "^3.1.1"
  }
}
